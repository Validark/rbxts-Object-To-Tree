{
	"name": "rbxts-object-to-tree",
	"version": "0.4.0",
	"private": true,
	"description": "Converts an object in Roblox Studio into its TS tree form.",
	"scripts": {
		"release-major": "npm version major && git push --tags",
		"release-minor": "npm version minor && git push --tags",
		"release-patch": "npm version patch && git push --tags",
		"build-plugin": "rbxtsc && rojo build --output rbxts-object-to-tree.rbxmx",
		"plugin-build": "npm run build-plugin",
		"install-rojo": "cargo install --force --git https://github.com/LPGhatguy/rojo.git"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/roblox-ts/demo-bundle.git"
	},
	"keywords": [],
	"author": "Validark",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/roblox-ts/demo-bundle/issues"
	},
	"homepage": "https://github.com/roblox-ts/demo-bundle#readme",
	"dependencies": {
		"@rbxts/cue": "^1.0.1",
		"@rbxts/delay-spawn-wait": "^2.0.0",
		"@rbxts/tween": "^1.0.4"
	},
	"devDependencies": {
		"@rbxts/compiler-types": "^1.1.1-types.3",
		"@rbxts/types": "^1.0.489",
		"@typescript-eslint/eslint-plugin": "^4.14.1",
		"@typescript-eslint/parser": "^4.14.1",
		"eslint": "^7.18.0",
		"eslint-config-prettier": "^7.2.0",
		"eslint-plugin-prettier": "^3.3.1",
		"eslint-plugin-roblox-ts": "0.0.24",
		"prettier": "^2.2.1",
		"roblox-ts": "^1.1.1",
		"typescript": "^4.1.3"
	}
}
